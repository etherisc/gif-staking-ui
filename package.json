{
  "name": "staking-ui",
  "version": "1.0.10",
  "private": true,
  "scripts": {
    "postinstall": "$npm_execpath run typechainDepegTypes && $npm_execpath run typechainRegistryTypes",
    "dev": "next dev",
    "prebuild": "./extract_version.sh",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "testLoop": "jest --watch",
    "test": "jest",
    "typechainDepegTypes": "typechain --target ethers-v5 --out-dir src/contracts/depeg-contracts './node_modules/@etherisc/depeg-contracts/build/contracts/**/*json'",
    "typechainRegistryTypes": "typechain --target ethers-v5 --out-dir src/contracts/registry-contracts './node_modules/@etherisc/registry-contracts/build/contracts/**/*json'"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.13.7",
    "@mui/material": "^5.13.6",
    "@mui/x-data-grid": "^6.9.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@web3modal/ethereum": "^2.6.2",
    "@web3modal/react": "^2.6.2",
    "abi-coder": "^4.1.1",
    "canvas-confetti": "^1.6.0",
    "dayjs": "^1.11.9",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.9",
    "ethers": "^5.7.2",
    "i18next": "^22.5.1",
    "moment": "^2.29.4",
    "next": "^13.4.9",
    "next-i18next": "^13.3.0",
    "nextjs-google-analytics": "^2.3.3",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-blockies": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.1.1",
    "typescript": "^5.1.6",
    "viem": "^1.2.10",
    "wagmi": "^1.3.8"
  },
  "devDependencies": {
    "@etherisc/depeg-contracts": "1.1.0",
    "@etherisc/registry-contracts": "1.1.0",
    "@hookform/devtools": "^4.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@typechain/ethers-v5": "^11.0.0",
    "@types/canvas-confetti": "^1.6.0",
    "@types/node": "^18.16.19",
    "@types/react": "^18.2.14",
    "@types/react-blockies": "^1.4.1",
    "@types/react-dom": "^18.2.6",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "typechain": "^8.2.0"
  }
}
