{
  "name": "staking-ui",
  "version": "1.1.10",
  "private": true,
  "scripts": {
    "postinstall": "$npm_execpath run typechainRegistryTypes && $npm_execpath run typechainGifInterfaceTypes",
    "dev": "next dev",
    "prebuild": "./extract_version.sh",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "testLoop": "jest --watch",
    "test": "jest",
    "typechainRegistryTypes": "typechain --target ethers-v5 --out-dir src/contracts/registry-contracts './node_modules/@etherisc/registry-contracts/build/contracts/**/*json'",
    "typechainGifInterfaceTypes": "typechain --target ethers-v5 --out-dir src/contracts/gif-interface-contracts './node_modules/@etherisc/gif-interface/build/contracts/**/*json'"
  },
  "dependencies": {
