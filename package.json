    "@typechain/ethers-v5": "^11.1.2",
    "@types/canvas-confetti": "^1.6.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^18.2.48",
    "@types/react-blockies": "^1.4.4",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
