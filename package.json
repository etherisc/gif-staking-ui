    "ethers": "^5.7.2",
    "i18next": "^22.5.1",
    "moment": "^2.30.1",
    "nanoid": "^5.0.4",
    "next": "14.0.4",
    "next-i18next": "^13.3.0",
    "nextjs-google-analytics": "^2.3.3",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-blockies": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-i18next": "^12.3.1",
    "react-redux": "^9.1.0",
    "redis": "^4.6.12",
